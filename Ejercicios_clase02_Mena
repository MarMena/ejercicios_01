Mariana-Mena:Desktop Mariana$ cd test
Mariana-Mena:test Mariana$ mkdir hello
Mariana-Mena:test Mariana$ cd hello
Mariana-Mena:hello Mariana$ mkdir mena
Mariana-Mena:hello Mariana$ cd ..
Mariana-Mena:test Mariana$ mkdir world
Mariana-Mena:test Mariana$ ls
hello	world
Mariana-Mena:test Mariana$ hello
-bash: hello: command not found
Mariana-Mena:test Mariana$ cd hello
Mariana-Mena:hello Mariana$ ls
mena

Ejercicio 07:

Mariana-Mena:hello Mariana$ ls
mena
Mariana-Mena:hello Mariana$ cd ..
Mariana-Mena:test Mariana$ cd world
Mariana-Mena:world Mariana$ echo "test" >> test_a.txt
Mariana-Mena:world Mariana$ ls
test_a.txt
Mariana-Mena:world Mariana$ echo "test" >> test_b.txt
Mariana-Mena:world Mariana$ ls
test_a.txt	test_b.txt
Mariana-Mena:world Mariana$ 

Ejercicio 08:

Mariana-Mena:world Mariana$ ls
test_a.txt	test_b.txt

Ejercicio 09:

Mariana-Mena:world Mariana$ rm test_a.txt 
Mariana-Mena:world Mariana$ ls
test_b.txt

Ejercicio 10:

Mariana-Mena:world Mariana$ cp test_b.txt ../hello/mena
Mariana-Mena:world Mariana$ cd ..
Mariana-Mena:test Mariana$ ls
hello	world
Mariana-Mena:test Mariana$ cd hello
Mariana-Mena:hello Mariana$ cd mena
Mariana-Mena:mena Mariana$ ls
test_b.txt
Mariana-Mena:mena Mariana$

